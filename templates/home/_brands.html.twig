<div class="row">
    {% set i = 1 %}
    {% for brand in brands %}
        {% if i % 5 == 1 %}
            <div id="conte">
        {% endif %}
        <a class="col" href="{{ path('research', {brand: brand}) }}">{{ brand }}</a>
        {% if i % 5 == 0 %}
            </div>
        {% endif %}
        {% set i = i + 1 %}
    {% endfor %}
    </div>
	<div class="vali mt-3">
		<a href="{{ path('research') }}">Toutes les marques</a>
	</div>
</div>