<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
																																																																				           and uncomment the following Encore helpers to start using Symfony UX #}
		{# <link rel="stylesheet" href="./bootstrap-5.0.1-dist/css/bootstrap.min.css"> #}
		<link rel="stylesheet" href="{{ '../../css/style.css' }}">
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.0/font/bootstrap-icons.css">
		<link rel="stylesheet" href="{{ 'css/style.vendre.css' }}">
		<link rel="stylesheet" href="{{ 'css/style.conseils.css' }}">
		<link rel="stylesheet" href="{{ '/css/style.carSale.css' }}">
		<link rel="stylesheet" href="{{ 'css/style.finance.css' }}">
		<link rel="stylesheet" href="{{ 'css/style.finance.css' }}">
		<link rel="stylesheet" href="{{ '/css/style.creditClassique.css' }}">


		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
		{% endblock %}

		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
			<script src="{{ '../js/script.js' }} "></script>
		{% endblock %}

		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

		{{ encore_entry_link_tags('app') }}
		{{ encore_entry_script_tags('app') | replace({'src=': 'defer src='}) | raw }}
	</head>
	<body>
		<div class="container">
			<nav class="navbar navbar-expand-lg navbar-dark" style="background-color: #1064ff">
				<div class="container-fluid">
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarSupportedContent">
						<ul class="navbar-nav me-auto mb-2 mb-lg-0">
							<li class="nav-item">
								<a href="{{ path('home') }} "><img src="{{ '../../img/logo11.png' }}" alt=""></a>
							</li>
							{% if app.user %}
								<li class="nav-item ms-3">
									<a class="nav-link active" href="{{ path('vendre') }}">Vendre</a>
								</li>
							{% else %}
								<li class="nav-item ms-3">
									<a class="nav-link active d-none" href="{{ path('vendre') }}">Vendre</a>
								</li>
							{% endif %}

							<li class="nav-item ms-3">
								<a class="nav-link active" href="{{ path('finance') }}">Finance</a>
							</li>
							<li class="nav-item ms-3">
								<a class="nav-link active" href="{{ path('conseils') }}">Conseil</a>
							</li>
							<li class="nav-item ms-3">
								<a class="nav-link active" href="{{ path('phone_book') }}">Annuaire</a>
							</li>
							<li class="nav-item ms-3">
								<a class="nav-link active" href="{{ path('research') }}">Recherche</a>
							</li>
							{# <li class="nav-item">
																													<a class="nav-link active" href="#">Prix</a>
																												</li> #}
							{% if  app.user %}
								<li class="nav-item ms-3">
									<a class="nav-link active" href="{{ path('add') }}">Annonce</a>
								</li>
								<li class="nav-item ms-3">
									<a class="nav-link active" href="{{ path('app_logout') }}">Déconnexion</a>
								</li>
								<li class="nav-item ms-3">
									<a href="{{ path('user_tab', {'id': app.user.id}) }}" class="nav-link active">Bonjour
										<span class="text-capitalize fw-bold">{{app.user.email|split('@')|slice(0, -1)|join}}</span>
										!</a>
								</li>
							{% endif %}
							{% if  not app.user %}
								<li class="nav-item ms-3">
									<a class="nav-link active" href="{{ path('app_register') }}">Inscription</a>
								</li>
								<li class="nav-item ms-3">
									<a class="nav-link active" href="{{ path('app_login') }}">Connexion</a>
								</li>
							{% endif %}

							{% if is_granted('ROLE_ADMIN') %}
								<li>
									<a class=" nav-link active" href="{{ path('admin')}}">Adminstration</a>
								</li>
							{% endif  %}


						</ul>
						{# <form class="d-flex">
																															<input class="form-control me-2" type="search" placeholder="Recherche" aria-label="Search">
																															<button class="btn btn-outline-success" type="submit">Recherche</button>
																														</form> #}
					</div>
				</div>
			</nav>
			{% block body %}{% endblock %}
			<footer>
				<div id="fot" class="row text-center">
					<div id="fot-un" class="col-lg-4">
						<a href="">Qui somme nous</a>
						<a href="">Condition générale</a>
						<a href="">Mention légale</a>
					</div>
					<div id="fot-deux" class="col-lg-4">
						<a href="">Charte rédactionnelle</a>
						<a href="">Charte cookies</a>
						<a href="">Devenir annonceur</a>
					</div>
					<div id="fot-trois" class="col-lg-4">
						<a href="">Nous contacter</a>
					</div>
				</div>
				<div id="fot-a" class="row text-xs-center text-sm-center text-md-center">
					<p class="col-lg-6 text-lg-start">&copy;2021 Colombbus</p>
					<div id="p1" class="col-lg-6 text-lg-end"></div>
				</div>
			</footer>
		</div>
	</body>
</html></head></html>
